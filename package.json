{
  "name": "@pixore/prettier-config",
  "version": "0.0.1",
  "description": "",
  "main": "index.js",
  "scripts": {
    "prettier": "prettier --write \"*.{json,md,js}\""
  },
  "keywords": [],
  "author": "Jose Albizures <jose@albizures.com",
  "license": "MIT",
  "dependencies": {
    "prettier": "^1.16.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx,json,md,js}": [
      "prettier --write",
      "git add"
    ]
  },
  "devDependencies": {
    "husky": "^1.3.1",
    "lint-staged": "^8.1.5"
  }
}
